{
  "name": "pkg-add-deps",
  "version": "0.0.0",
  "description": "Add dependencies to package.json without install.",
  "bin": "cli.js",
  "scripts": {
    "lint": "eslint --ext js,md .",
    "test": "jest"
  },
  "dependencies": {
    "libnpm": "^2.0.1",
    "pacote": "^9.3.0",
    "read-pkg": "^4.0.1",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "eslint": "^5.11.1",
    "eslint-config-concise": "^0.35.0",
    "eslint-config-concise-jest": "^0.35.0",
    "execa": "^1.0.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "prettier-eslint-cli": "^4.7.1"
  },
  "repository": "ratson/pkg-add-deps",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "files": [
    "*.md",
    "index.js",
    "cli.js"
  ],
  "keywords": [],
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "jest"
    }
  }
}
